{
    "compilerOptions": {
        "types": [
            "node"
        ],
        "lib": [
            "esnext"
        ],
        "target": "es6",
        "moduleResolution": "node",
        "module": "CommonJS",
        "outDir": "./out",
        "strict": true,
        "esModuleInterop": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true
    },
    "include": [
        "src/**/*"
    ],
    "exclude": [
        "node_modules",
        "**/*.spec.ts"
    ],
}